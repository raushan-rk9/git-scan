<ul class="list-group">
  <li class="list-group-item active">
    <h5>
      <%= "Id: #{@github_access.id}" %>
    </h5>
  </li>
  <li class="list-group-item">
    <p>
      <strong>Username:</strong>
      <%= @github_access.username %>
    </p>
    
    <p>
      <strong>Password:</strong>
      <%= @github_access.display_password %>
    </p>
    
    <p>
      <strong>Token:</strong>
      <%= @github_access.display_token %>
    </p>
    
    <p>
      <strong>Repository URL:</strong>
      <%= @github_access.repository_url %>
    </p>
    
    <p>
      <strong>User:</strong>
      <%= @github_access.user.email %>
    </p>
  </li>
</ul>

<%= link_to 'Edit', edit_github_access_path(@github_access), class: 'btn' %>
<%= link_to 'Back', go_back_path(1),  onclick: 'validateBack(1, this)', class: 'btn' %>

<% if @undo_path.present? && !session[:archives_visible].present? %>
  <%= link_to 'Undo', @undo_path, class: 'btn', title: @undo_message %>
<% end %>

<% if @redo_path.present? && !session[:archives_visible].present? %>
  <%= link_to 'Redo', @redo_path, class: 'btn', title: @redo_message %>
<% end %>

