<style>
.text
{
  font-family: 'courier';
}
</style>

<h1>
  Diff Source Code Files
</h1>
<p>
  <div class='text'>
    <% @lines.each do |line| %>
      <%= line.gsub(' ', "&nbsp").html_safe %><br>
    <% end %>
  </div>
</p>

<%= link_to 'Back', go_back_path(1),  onclick: 'validateBack(1, this)', class: 'btn' %>
