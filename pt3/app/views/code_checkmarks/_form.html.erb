<%= simple_form_for(@code_checkmark) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :checkmark_id %>
    <%= f.association :source_code %>
    <%= f.input :filename %>
    <%= f.input :line_number %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
